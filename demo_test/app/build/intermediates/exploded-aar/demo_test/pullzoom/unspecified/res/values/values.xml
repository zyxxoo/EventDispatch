<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <declare-styleable name="MyPullToZoom"><attr format="dimension|reference" name="rootview_top"/><attr format="dimension|reference" name="zoomview_height"/></declare-styleable>
    <declare-styleable name="PullToZoomView"><attr format="reference" name="headerView"/><attr format="reference" name="contentView"/><attr format="reference" name="zoomView"/><attr format="boolean" name="isHeaderParallax"/></declare-styleable>
    <declare-styleable name="pull"><attr format="reference|integer" name="header"/><attr format="reference|dimension" name="maxHeaderHeight"/><attr format="reference|dimension" name="minHeaderHeight"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="bottom_height">40dp</dimen>
    <dimen name="default_pullzoom_height">60dp</dimen>
    <dimen name="default_pullzoom_top">180dp</dimen>
    <item name="tag_type" type="id"/>
    <string name="app_name">Demo</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
</resources>